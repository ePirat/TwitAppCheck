\u300eMY KB\u300f\n\ub098\uc758 \uae30\ubcf8\uc815\ubcf4\uc640 KB\uc2a4\ud0c0\ud074\ub7fd \ub4f1\uae09, \ud53c\uc2f1\ubc29\uc9c0 \uc774\ubbf8\uc9c0, \ubcf4\uc548\uc218\uc900\uc870\ud68c, \uad00\uc2ec\uc0c1\ud488 \ubaa9\ub85d \ubc0f \uc774\ubca4\ud2b8 \uc751\ubaa8\ub0b4\uc5ed, \ub9cc\uae30\uc77c\ub3c4\ub798 \ud604\ud669 \ub4f1\uc744 \ud55c\ub208\uc5d0 \ud655\uc778\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4.\n\n\u2462 \uc0c1\ud488\uac00\uc785\uc5d0\uc11c \uc0c1\ub2f4 \ubc0f \uad00\ub9ac\uae4c\uc9c0!\u300e\uae08\uc735\uc13c\ud130\u300f\n\ub098\uc5d0\uac8c \ub9de\ub294 \uc790\uc0b0 \uc9c4\ub2e8 \ubc0f \uc804\ubb38\uc801\uc778 \uae08\uc735\uc0c1\ud488 \uc815\ubcf4, \uadf8\ub9ac\uace0 \ub098\uc5d0\uac8c \ud544\uc694\ud55c \ud14c\ub9c8\ubcc4 \uc0c1\ud488 \u2028\ucd94\ucc9c\ubd80\ud130 \uc2a4\ub9c8\ud2b8 \uc0c1\ub2f4\uae4c\uc9c0, \uc0c1\ud488\ubcc4 \ucf00\uc5b4\uc11c\ube44\uc2a4\uc640 \uc790\uc0b0 \uad00\ub9ac\ub97c \uacbd\ud5d8\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4.\n\n\u2463 \ubaa8\ub4e0 \uc11c\ube44\uc2a4\uac00 \ud55c \ud654\uba74\uc5d0~\u300e\uc804\uccb4\uba54\ub274\u300f\n\uc804\uccb4\uba54\ub274\ub97c \ud130\uce58(\uc2ac\ub77c\uc774\ub529)\ud558\uba74 \ub2e4\uc591\ud55c \uae08\uc735\uc11c\ube44\uc2a4\ub85c \uc9c1\uc811 \uc774\ub3d9\ud560 \uc218 \uc788\uace0, \uc2a4\ub9c8\ud2b8\uc54c\ub9ac\ubbf8\ub97c \ud1b5\ud574 \uc0c8\ub85c\uc6b4 \uba54\uc2dc\uc9c0(Push\uc54c\ub9bc)\uc640 \ud658\uacbd\uc124\uc815\uc744 \uc774\uc6a9\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4.\n\n\n\u25a0 \uc0c8\ub85c\uc6cc\uc9c4 \uc11c\ube44\uc2a4 \n\u2460 \ud55c\uce35 \uac15\ud654\ub41c \ubcf4\uc548 : \uc774\uc911\ub85c\uadf8\uc778 \ubc29\uc9c0, \ub098\uc758 \ubcf4\uc548\uc218\uc900, \uc571\uc704\ubcc0\uc870 \ubc29\uc9c0/\ud0d0\uc9c0,\ud574\ud0b9\uc758\uc2ec\uac70\ub798 \ucc28\ub2e8, \uacc4\uc88c\uc911\uc2ec \ud648\ud654\uba74\uc73c\ub85c \ud53c\uc2f1\uc571\uacfc\uc758 \uc2dd\ubcc4 \n\u2461 \uc778\uc99d\uc11c \ubcf5\uc0ac\uc808\ucc28 \uac04\uc18c\ud654 : \uc2a4\ub9c8\ud2b8\uae30\uae30\uac04\uc5d0 \uc778\uc99d